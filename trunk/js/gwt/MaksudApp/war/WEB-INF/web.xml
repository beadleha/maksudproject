<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

	<servlet>
		<servlet-name>demo_sign</servlet-name>
		<servlet-class>org.maksud.gwt.app.maksudapp.server.servlets.demo.SignGuestbookServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>demo_sign</servlet-name>
		<url-pattern>/demo_sign</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>demo_mail</servlet-name>
		<servlet-class>org.maksud.gwt.app.maksudapp.server.servlets.demo.JavaMailDemo</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>demo_mail</servlet-name>
		<url-pattern>/demo_mail</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>demo_test</servlet-name>
		<servlet-class>org.maksud.gwt.app.maksudapp.server.servlets.demo.TestServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>demo_test</servlet-name>
		<url-pattern>/demo_test</url-pattern>
	</servlet-mapping>



	<servlet>
		<servlet-name>fileupload</servlet-name>
		<servlet-class>org.maksud.gwt.app.maksudapp.server.servlets.FileUploadServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>fileupload</servlet-name>
		<url-pattern>/fileupload</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>org.maksud.gwt.app.maksudapp.server.servlets.Login</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ActivateUser</servlet-name>
		<servlet-class>org.maksud.gwt.app.maksudapp.server.servlets.ActivateUser</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ActivateUser</servlet-name>
		<url-pattern>/activate</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>UserRegistration</servlet-name>
		<servlet-class>org.maksud.gwt.app.maksudapp.server.servlets.UserRegistration</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserRegistration</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>demo_data</servlet-name>
		<servlet-class>org.maksud.gwt.app.maksudapp.server.servlets.demo.DataServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>demo_data</servlet-name>
		<url-pattern>/demo_data</url-pattern>
	</servlet-mapping>


	<welcome-file-list>
		<welcome-file>/index.html</welcome-file>
		<welcome-file>/index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>BasicRPC</servlet-name>
		<servlet-class>org.maksud.gwt.app.maksudapp.server.BasicRPCImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>BasicRPC</servlet-name>
		<url-pattern>/maksudapp/BasicRPC</url-pattern>
	</servlet-mapping>

</web-app>